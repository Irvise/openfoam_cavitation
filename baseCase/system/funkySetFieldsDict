FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "Imploding bubble";
    instance        "system";
    local           "";

    class           dictionary;
    object          funkySetFieldsDict;
}

expressions
(
	ambientFluidp
	{
		field p;
		expression	"100000"; // 100_000 [Pa]
		keepPatches false;
	}
	ambientFluidU
	{
		field U;
		expression	"vector(0, 0, 0)";
		keepPatches false;
	}
	ambientFluidalpha.vapour
	{
		field alpha.vapour;
		expression	"0";
		keepPatches false;
	}
	ambientFluidk
	{
		field k;
		expression	"0";
		keepPatches false;
	}
	ambientFluidomega
	{
		field omega;
		expression	"0";
		keepPatches false;
	}
	ambientFluidrho
	{
		field rho;
		expression	"997.2987703166269"; // Rho at 100_000 [Pa] and T at 297.15 [K]
		keepPatches false;
    }
	ambientFluidnut
	{
		field nut;
		expression	"0";
		keepPatches false;
    }

	bubblep
	{
		field p;
		expression	"2000"; // 2000 [Pa]
		condition "pow(pos().x,2) + pow(pos().y-0.1,2) + pow(pos().z-0,2)-pow(5e-2,2) < 0" ;
		keepPatches true;
    }
	bubbleU
	{
		field U;
		expression	"vector(0, 0, 0)";
		condition "pow(pos().x,2) + pow(pos().y-0.1,2) + pow(pos().z-0,2)-pow(5e-2,2) < 0" ;
		keepPatches true; 
    }
	bubblealpha.vapour
	{ 	
		field alpha.vapour;
		expression	"1";
		condition "pow(pos().x,2) + pow(pos().y-0.1,2) + pow(pos().z-0,2)-pow(5e-2,2) < 0" ;
		keepPatches true; 
    }
	bubblek
	{
		field k;
		expression	"0";
		condition "pow(pos().x,2) + pow(pos().y-0.1,2) + pow(pos().z-0,2)-pow(5e-2,2) < 0" ;
		keepPatches true; 
    }
	bubbleomega
	{		
		field omega;
		expression	"0";
		condition "pow(pos().x,2) + pow(pos().y-0.1,2) + pow(pos().z-0,2)-pow(5e-2,2) < 0" ;
		keepPatches true;
    }
	bubblerho
	{
		field rho;
		expression	"0.014598794912239475"; // Rho at 2000 [Pa] and T at 297.15 [K]
		condition "pow(pos().x,2) + pow(pos().y-0.1,2) + pow(pos().z-0,2)-pow(5e-2,2) < 0" ;
		keepPatches true;   	

	}
	bubblenut
