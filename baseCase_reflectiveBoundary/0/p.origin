    {
      type            freestreamPressure;
      freestreamValue uniform 1e5;
      supersonic      false;
      value           uniform 1e5;
    }
  outlet
    {
      type            freestreamPressure;
      freestreamValue uniform 1e5;
      supersonic      false;
      value           uniform 1e5;
    }
  wall
    {
      type            freestreamPressure;
      freestreamValue uniform 1e5;
      supersonic      false;
      value           uniform 1e5;
