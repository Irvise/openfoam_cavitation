  inlet
    {
      type inletOutlet;
      value uniform 1e5;
      inletValue uniform 1e5;
    }
  outlet
    {
      type inletOutlet;
      value uniform 1e5;
      inletValue uniform 1e5;
    }
  wall
    {
      type inletOutlet;
      value uniform 1e5;
      inletValue uniform 1e5;
    }
  
	axis
	{
		type empty; 

	}

	wedgePatch1
	{
		type wedge; 

	}
	
	wedgePatch2
	{
		type wedge; 

	}

}
