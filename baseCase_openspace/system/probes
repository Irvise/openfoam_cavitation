fields (p);
probeLocations
(
    (0 0.005 0.005)
    (0 0.015 0.015)
    (0 0.034 0.0333)
    // near to the center
    (0 0.0005 0.0005)
    // bubble surface 
    (0 0.03 0.03)
);

